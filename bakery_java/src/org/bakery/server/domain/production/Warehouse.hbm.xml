<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="org.bakery.server.domain.production.Warehouse"
		table="product_warehouses">
		<id name="id"
			column="product_warehouse_id">
			<generator class="increment"/>
		</id>				
		<property
			name="name"
			column="product_warehouse_name"/>
		<property name="active"
			  type="integer"
			  column="product_warehouse_active"/>					
		<many-to-one
			name="owner"
			column="product_warehouse_owner"/>
			
		<many-to-one
			name="address"
			column="product_warehouse_address"/>	

	</class>
</hibernate-mapping>